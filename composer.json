{
  "name": "leadingfellows/multiserver_guzzle",
  "description": "PSR-7 multi-server guzzle client",
  "type": "library",
  "license": "GPL-3.0-or-later",
  "authors": [
    {
      "name": "Mikael MEULLE",
      "email": "mikael.meulle@leadingfellows.com",
      "homepage": "https://github.com/mikaelmeulle"
    }
  ],
  "require": {
    "php": ">=7.0",
    "ext-curl": "*",
    "guzzlehttp/psr7": "^1.6",
    "guzzlehttp/guzzle": "^7.0 || ^6.3"
  },
  "require-dev": {
      "phpstan/phpstan" : "^1.2",
      "symfony/process" : "^5.4",
      "phpunit/phpunit": "^9.5",
      "escapestudios/symfony2-coding-standard": "^3.12",
      "squizlabs/php_codesniffer": "^3.6"
  },
  "autoload": {
    "psr-4": {
      "leadingfellows\\multiserver_guzzle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "leadingfellows\\MultiGuzzleTests\\": "tests/phpunit/src"
    }
  },
  "scripts": {
        "test": "vendor/bin/phpunit",
        "phpstan": "vendor/bin/phpstan",
        "phpcs": "vendor/bin/phpcs"
   },
  "keywords": [
    "framework",
    "http",
    "rest",
    "web service",
    "curl",
    "client",
    "guzzle",
    "HTTP client",
    "PSR-7"
  ]
}
